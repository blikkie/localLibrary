{
  "extends": "airbnb-base",
  "plugins": [
    "import"
  ],
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "next"
      }
    ],
    "no-console": "off",
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "func-names": "off",
    "prefer-destructuring": "off",
    "consistent-return": "off",
    "no-plusplus":"off",
    "no-param-reassign":"off",
    "no-shadow":"off"
  }
}